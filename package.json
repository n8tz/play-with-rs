{
  "name": "www.mamasound.fr",
  "version": "2.0.0",
  "private": true,
  "main": "./dist/App.server.js",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "libsPath": "./libs",
      "vars": {
        "production": true,
        "resolve": {
          "alias": {
            "material-ui": "@material-ui/core"
          }
        },
        "webpackPatch": {
          "optimization": {
            "usedExports": true
          }
        }
      },
      "extend": [
        "wpi-forms",
        "wpi-xsl",
        "wpi-mongo",
        "wpi-react-rs-sass-ssr"
      ]
    },
    "devClient": {
      "rootFolder": "App",
      "basedOn": "default",
      "libsPath": "./libs",
      "vars": {
        "webpackPatch": {
          "stats": "errors-only",
          "devServer": {
            "compress": true,
            "disableHostCheck": true
          },
          "resolve": {
            "alias": {
              "material-ui": "@material-ui/core"
            }
          },
          "watchOptions": {
            "aggregateTimeout": 300,
            "poll": 1000
          }
        }
      },
      "extend": [
        "wpi-forms",
        "wpi-xsl",
        "wpi-mongo",
        "wpi-react-rs-sass-ssr"
      ]
    },
    "api": {
      "rootFolder": "App",
      "libsPath": "./libs",
      "vars": {
        "externals": true,
        "webpackPatch": {
          "stats": "errors-only",
          "devtool": "eval-source-map",
          "resolve": {
            "alias": {
              "material-ui": "@material-ui/core"
            }
          },
          "watchOptions": {
            "aggregateTimeout": 300,
            "poll": 1000
          }
        }
      },
      "extend": [
        "wpi-forms",
        "wpi-xsl",
        "wpi-mongo",
        "wpi-react-rs-sass-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi&&wpi :api",
    "dev-api": "wpi :api -w",
    "start-dev": "wpi :api&&concurrently \"wpi-dev-server :devClient --hot --host 0.0.0.0 \" \"wpi :api -w\" \"sleep 3&&nodemon -L --watch ./dist/App.server.js --inspect=[::]:9229 ./dist/App.server.js  -p 9701\"",
    "start": "node ./dist/App.server.js  -p 80",
    "runBrowser": "opener http://localhost:8080",
    "docker_start": "opener docker-compose up&",
    "docker_down": "docker-compose down",
    "docker_kill": "docker-compose kill",
    "docker_ash": "opener docker-compose exec app ash -c \"cd /home/app&&ash\"&",
    "docker_build": "opener docker-compose exec app ash -c \"cd /home/app&&npm run build\"&",
    "docker_restoreDb": "opener docker-compose exec app ash -c \"cd /home/app&&mongorestore --uri ${mongoUrl}\"&",
    "docker_clear": "docker-compose down -v",
    "docker_runBrowser": "opener http://mamasound.localhost&"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^12.1.0",
    "@ckeditor/ckeditor5-react": "^1.1.3",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "ag-grid": "^18.1.2",
    "ag-grid-community": "^20.1.0",
    "ag-grid-react": "^20.1.0",
    "autoresponsive-react": "^1.1.31",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.18.2",
    "concurrently": "^4.1.0",
    "d3-ease": "^1.0.5",
    "debounce": "^1.2.0",
    "debug-logger": "^0.4.1",
    "dot-tpl-loader": "^1.2.1",
    "express": "^4.16.3",
    "express-basic-auth": "^1.1.6",
    "extract-hoc": "0.0.5",
    "geolib": "^2.0.24",
    "greenlock-express": "^2.6.8",
    "helmet": "^3.16.0",
    "https": "^1.0.0",
    "is": "^3.3.0",
    "jmespath": "^0.15.0",
    "json-query": "^2.2.2",
    "knex-schemer": "^0.1.17",
    "leaflet": "^1.4.0",
    "leaflet-pulse-icon": "^0.1.0",
    "material-ui-chip-input": "^1.0.0-beta.15",
    "memoize-one": "^5.0.4",
    "minimist": "^1.2.0",
    "moment": "^2.23.0",
    "mongodb": "^2.2.36",
    "multer": "^1.4.1",
    "node-watch": "^0.6.0",
    "nodemon": "^1.18.7",
    "opener": "^1.5.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-add-to-calendar": "^0.1.5",
    "react-caipi-slider": "^1.0.4",
    "react-dom": "^16.2.0",
    "react-dropzone-component": "^3.2.0",
    "react-facebook": "^8.0.4",
    "react-grid-layout": "^0.16.6",
    "react-infinite-scroller": "^1.2.4",
    "react-inheritable-contextmenu": "^1.5.1",
    "react-leaflet": "^2.2.1",
    "react-rescope": "^16.0.8",
    "react-rnd": "^9.0.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-rtween": "^1.4.0",
    "react-select": "^2.4.1",
    "react-share": "^2.4.0",
    "react-slick": "^0.23.2",
    "react-swipeable-views": "^0.13.1",
    "react-tetris": "^0.1.0",
    "redis": "^2.8.0",
    "request": "^2.85.0",
    "rscopes": "^16.0.2",
    "rtween": "^1.2.1",
    "shortid": "^2.2.14",
    "slick-carousel": "^1.8.1",
    "speakingurl": "^14.0.1",
    "superagent": "^3.8.2",
    "uniforms": "^1.31.1",
    "uniforms-material": "^1.31.1",
    "url-loader": "^1.1.2",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "wpi-react-rs-sass-ssr": "^1.0.5",
    "webpack-inherit": "^1.5.2"
  }
}
