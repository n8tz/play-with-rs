  "scripts": {
    "build": "wpi :client&&wpi :admin&&wpi :api",
    "dev-api": "wpi :devApi -w",
    "start-dev": "wpi :devApi&&concurrently \"wpi-dev-server :devClient --hot --host 0.0.0.0 \" \"wpi :devApi -w\" \"sleep 3&&nodemon -L --watch ./dist/api/App.server.js --inspect=[::]:9229 ./dist/api/App.server.js  -p 9701\"",
    "start": "node ./dist/api/App.server.js  -p 8080",
    "runBrowser": "opener http://localhost:8080",
    "docker_start": "cd dockers/dev&&opener docker-compose up",
    "docker_restart": "cd dockers/dev&&docker-compose kill&&opener docker-compose up",
    "docker_restartClean": "(rm package-lock.json||sleep 0) &&cd dockers/dev&&docker-compose down -v&& opener docker-compose up",
    "docker_down": "cd dockers/dev&&docker-compose down",
    "docker_kill": "cd dockers/dev&&docker-compose kill",
    "docker_ash": "cd dockers/dev&&opener docker-compose exec app ash -c \"cd /home/app&&ash\"",
    "docker_build": "cd dockers/dev&&opener docker-compose exec app ash -c \"cd /home/app&&npm run build\"",
    "docker_restoreDb": "cd dockers/dev&&opener docker-compose exec app ash -c \"cd /home/app&&mongorestore --uri \\$MONGO_URI\"",
    "docker_runBrowser": "opener http://mamasound.localhost&"
  },
