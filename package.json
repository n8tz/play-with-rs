{
  "name": "mama-importer",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "./dist/App.server.js",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "libsPath": "./libs",
      "vars": {
        "production": true
      },
      "extend": [
        "wpi-react-rs-sass-ssr"
      ]
    },
    "devClient": {
      "rootFolder": "App",
      "basedOn": "default",
      "libsPath": "./libs",
      "vars": {
        "webpackPatch": {
          "watchOptions": {
            "aggregateTimeout": 300,
            "poll": 1000
          }
        }
      },
      "extend": [
        "wpi-react-rs-sass-ssr"
      ]
    },
    "api": {
      "rootFolder": "App",
      "libsPath": "./libs",
      "vars": {
        "externals": true,
        "webpackPatch": {
          "devtool": "inline",
          "watchOptions": {
            "aggregateTimeout": 300,
            "poll": 1000
          }
        }
      },
      "extend": [
        "wpi-react-rs-sass-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi&&wpi :api",
    "dev-api": "wpi :api -w",
    "docker_start": "start docker-compose up&",
    "docker_down": "docker-compose down",
    "docker_ash": "start docker-compose exec app ash -c \"cd /home/app&&ash\"&",
    "docker_restoreDb": "start docker-compose exec app ash -c \"cd /home/app&&mongorestore --uri ${mongoUrl}\"&",
    "start-dev": "wpi :api&&concurrently \"wpi-dev-server :devClient --hot --host 0.0.0.0 \" \"wpi :api -w\" \"sleep 3&&nodemon -L --watch ./dist/App.server.js --inspect=[::]:9229 ./dist/App.server.js  -p 9701\"",
    "start": "node ./dist/App.server.js  -p 8080",
    "runBrowser": "start http://localhost:8080"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "ag-grid": "^18.1.2",
    "ag-grid-community": "^20.1.0",
    "ag-grid-react": "^20.1.0",
    "body-parser": "^1.18.2",
    "concurrently": "^4.1.0",
    "debounce": "^1.2.0",
    "debug-logger": "^0.4.1",
    "dot-tpl-loader": "^1.2.1",
    "express": "^4.16.3",
    "extract-hoc": "0.0.5",
    "is": "^3.3.0",
    "js-file-download": "^0.4.4",
    "knex-schemer": "^0.1.17",
    "minimist": "^1.2.0",
    "moment": "^2.23.0",
    "mongodb": "^3.1.13",
    "multer": "^1.4.1",
    "node-watch": "^0.6.0",
    "nodemon": "^1.18.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone-component": "^3.2.0",
    "react-rnd": "^9.0.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.4.1",
    "request": "^2.85.0",
    "shortid": "^2.2.14",
    "speakingurl": "^14.0.1",
    "superagent": "^3.8.2",
    "url-loader": "^1.1.2",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "wpi-react-rs-sass-ssr": "^1.0.3",
    "webpack-inherit": "^1.4.9"
  }
}
