{
  "default": {
    "rootFolder": "App",
    "libsPath": "./packages"
  },
  "client": {
    "rootFolder": "App",
    "libsPath": "./packages",
    "basedOn": "default",
    "vars": {
      "targetDir": "dist/www",
      "extractCss": true,
      "production": true,
      "DefinePluginCfg": {
        "__IS_DEV__": false,
        "__IS_ADMIN__": false,
        "__IS_SERVER__": false
      },
      "resolve": {
        "alias": {
          "material-ui": "@material-ui/core"
        }
      },
      "babelInclude": "(ansi\\-regex|strip\\-ansi)",
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 6, ios >= 6, chrome >= 52"
        },
        "loose": true
      },
      "webpackPatch": {
        "mode": "production"
      }
    },
    "extend": [
      "wpi-auth",
      "wpi-forms",
      "wpi-medias",
      "wpi-mongo",
      "wpi-react-rs-sass-ssr"
    ]
  },
  "admin": {
    "rootFolder": "App",
    "libsPath": "./packages",
    "basedOn": "default",
    "vars": {
      "targetDir": "dist/admin",
      "extractCss": true,
      "production": true,
      "babelInclude": "(ansi\\-regex|strip\\-ansi|ckeditor)",
      "DefinePluginCfg": {
        "__IS_DEV__": false,
        "__IS_ADMIN__": true,
        "__IS_SERVER__": false
      },
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 6, ios >= 6, chrome >= 52"
        },
        "loose": true
      },
      "resolve": {
        "alias": {
          "material-ui": "@material-ui/core"
        }
      },
      "webpackPatch": {
        "mode": "production"
      }
    },
    "extend": [
      "wpi-auth",
      "wpi-admin",
      "wpi-forms",
      "wpi-medias",
      "wpi-xsl",
      "wpi-mongo",
      "wpi-react-rs-sass-ssr"
    ]
  },
  "devClient": {
    "rootFolder": "App",
    "basedOn": "default",
    "libsPath": "./packages",
    "vars": {
      "DefinePluginCfg": {
        "__IS_DEV__": true,
        "__IS_ADMIN__": true,
        "__IS_SERVER__": false
      },
      "babelInclude": "(ansi\\-regex|strip\\-ansi|tui\\-image\\-editor)",
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 6, ios >= 6, chrome >= 52"
        },
        "loose": true
      },
      "webpackPatch": {
        "stats": "errors-only",
        "devServer": {
          "public": "mamasound.localhost",
          "compress": true,
          "disableHostCheck": true
        },
        "resolve": {
          "alias": {
            "material-ui": "@material-ui/core"
          }
        },
        "watchOptions": {
          "aggregateTimeout": 300,
          "poll": 1000
        }
      }
    },
    "extend": [
      "wpi-dev-tools",
      "wpi-auth",
      "wpi-admin",
      "wpi-forms",
      "wpi-medias",
      "wpi-xsl",
      "wpi-mongo",
      "wpi-react-rs-sass-ssr"
    ]
  },
  "api": {
    "rootFolder": "App",
    "libsPath": "./packages",
    "vars": {
      "targetDir": "dist/api",
      "externals": true,
      "production": true,
      "DefinePluginCfg": {
        "__IS_DEV__": false,
        "__IS_ADMIN__": true,
        "__IS_SERVER__": true
      },
      "webpackPatch": {
        "stats": "errors-only",
        "resolve": {
          "alias": {
            "material-ui": "@material-ui/core"
          }
        },
        "watchOptions": {
          "aggregateTimeout": 300,
          "poll": 1000
        }
      }
    },
    "extend": [
      "wpi-auth",
      "wpi-admin",
      "wpi-forms",
      "wpi-medias",
      "wpi-xsl",
      "wpi-mongo",
      "wpi-react-rs-sass-ssr"
    ]
  },
  "devApi": {
    "rootFolder": "App",
    "libsPath": "./packages",
    "basedOn": "api",
    "vars": {
      "targetDir": "dist/api",
      "externals": true,
      "DefinePluginCfg": {
        "__IS_DEV__": true,
        "__IS_ADMIN__": true,
        "__IS_SERVER__": true
      },
      "webpackPatch": {
        "stats": "errors-only",
        "resolve": {
          "alias": {
            "material-ui": "@material-ui/core"
          }
        },
        "watchOptions": {
          "aggregateTimeout": 300,
          "poll": 1000
        }
      }
    },
    "extend": [
      "wpi-dev-tools",
      "wpi-auth",
      "wpi-admin",
      "wpi-forms",
      "wpi-medias",
      "wpi-xsl",
      "wpi-mongo",
      "wpi-react-rs-sass-ssr"
    ]
  }
}