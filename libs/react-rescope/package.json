{
  "name": "react-rescope",
  "version": "16.0.1",
  "description": "ReScope state management system for React",
  "repository": "https://github.com/n8tz/react-rescope.git",
  "main": "dist/ReactRS.js",
  "license": "MIT",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "rootAlias": "ReactRS",
        "externals": true,
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "staging": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "rootAlias": "ReactRS",
        "externals": true,
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "samples": {
      "rootFolder": "src",
      "basedOn": "ComponentSamples",
      "vars": {
        "rootAlias": "ReactRS",
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi && wpi :samples",
    "staging": "wpi :staging && wpi :samples",
    "dev": "wpi-dev-server :samples --hot",
    "runBrowser": "start http://localhost:8080"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react-dom": "^16.3.2",
    "react": "^16.3.2"
  },
  "dependencies": {
    "is": "^3.2.1",
    "@babel/runtime": "^7.3.1",
    "rescope": "^4.3.0"
  },
  "devDependencies": {
    "wpi-react-hmr-ssr": "^1.1.4",
    "webpack-inherit": "^1.4.10",
    "jsdom": "^11.10.0",
    "mocha": "^3.5.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
