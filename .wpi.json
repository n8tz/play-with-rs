{
  "default": {
    "rootFolder": "App",
    "libsPath": "./libs",
    "vars": {
      "production": true,
      "resolve": {
        "alias": {
          "material-ui": "@material-ui/core"
        }
      },
      "webpackPatch": {
        "optimization": {
          "usedExports": true
        }
      }
    },
    "extend": [
      "wpi-forms",
      "wpi-xsl",
      "wpi-mongo",
      "wpi-react-rs-sass-ssr"
    ]
  },
  "devClient": {
    "rootFolder": "App",
    "basedOn": "default",
    "libsPath": "./libs",
    "vars": {
      "webpackPatch": {
        "stats": "errors-only",
        "resolve": {
          "alias": {
            "material-ui": "@material-ui/core"
          }
        },
        "watchOptions": {
          "aggregateTimeout": 300,
          "poll": 1000
        }
      }
    },
    "extend": [
      "wpi-forms",
      "wpi-xsl",
      "wpi-mongo",
      "wpi-react-rs-sass-ssr"
    ]
  },
  "api": {
    "rootFolder": "App",
    "libsPath": "./libs",
    "vars": {
      "externals": true,
      "webpackPatch": {
        "stats": "errors-only",
        "devtool": "eval-source-map",
        "resolve": {
          "alias": {
            "material-ui": "@material-ui/core"
          }
        },
        "watchOptions": {
          "aggregateTimeout": 300,
          "poll": 1000
        }
      }
    },
    "extend": [
      "wpi-forms",
      "wpi-xsl",
      "wpi-mongo",
      "wpi-react-rs-sass-ssr"
    ]
  }
}